@model IEnumerable<WeatherData>


   <table class="table table-active">
       <thead>
           <tr>
            <th>Date</th>
            <th>Summary</th>
            <th>Temperature C</th>
           </tr>
       </thead>
       <tbody>
           @foreach (var item in Model)
        {
            <tr>
                <td>@item.Date.ToString()</td>
                <td>@item.Summary</td>
                <td>@item.TemperatureC</td>
            </tr>
        }
       </tbody>
   </table>